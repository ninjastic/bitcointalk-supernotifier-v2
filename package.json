{
  "name": "supernotifier2",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "release": "standard-version",
    "start:scrapper": "cross-env NODE_ENV=production node dist/shared/infra/bull/workers/scrapper.js",
    "start:server": "cross-env NODE_ENV=production node dist/shared/infra/http/server.js",
    "start:telegram": "cross-env NODE_ENV=production node dist/shared/infra/telegram/index.js",
    "start:checker": "cross-env NODE_ENV=production node dist/shared/infra/bull/workers/checker.js",
    "dev:scrapper": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/shared/infra/bull/workers/scrapper.ts | pino-pretty",
    "dev:server": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/shared/infra/http/server.ts | pino-pretty",
    "dev:telegram": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/shared/infra/telegram/index.ts | pino-pretty",
    "dev:checker": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/shared/infra/bull/workers/checker.ts | pino-pretty",
    "dev:es:upload:posts": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/shared/tools/elasticsearch/upload-posts.ts",
    "dev:loyce:archive": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/shared/tools/loyce/scraper-archive.ts",
    "postinstall": "typesync",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migrate": "cross-env NODE_ENV=production typeorm migration:run",
    "dev:migrate": "cross-env NODE_ENV=development ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "post:add": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/shared/tools/forum/manual-post.ts",
    "post:censor": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/shared/tools/forum/censor-post.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/body-parser": "^1.19.0",
    "@types/bull": "^3.14.0",
    "@types/cheerio": "^0.22.21",
    "@types/cors": "^2.8.7",
    "@types/crypto-js": "^4.0.2",
    "@types/decompress": "^4.2.3",
    "@types/escape-html": "^1.0.0",
    "@types/eslint": "^7.2.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.7",
    "@types/express-rate-limit": "^5.1.0",
    "@types/fs-extra": "^9.0.1",
    "@types/helmet": "^0.0.48",
    "@types/ioredis": "^4.17.2",
    "@types/node": "^8.0.29",
    "@types/node-fetch": "^2.5.7",
    "@types/pg": "^7.14.4",
    "@types/pino": "^6.3.0",
    "@types/pino-http": "^5.0.4",
    "@types/pluralize": "^0.0.29",
    "@types/prettier": "^2.0.2",
    "@types/socket.io": "^2.1.11",
    "@types/standard-version": "^7.0.0",
    "@types/underscore": "^1.10.22",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "pino-pretty": "^4.1.0",
    "prettier": "^2.0.5",
    "standard-version": "^9.0.0",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.5",
    "typesync": "^0.7.0"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.9.0",
    "axios": "^0.19.2",
    "bitcoin-address-validation": "^1.0.2",
    "body-parser": "^1.19.0",
    "bodybuilder": "^2.3.0",
    "bull": "^3.18.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.15.0",
    "decompress": "4.2.0",
    "decompress-tarbz2": "^4.1.1",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "enquirer": "^2.3.6",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "form-data": "^3.0.0",
    "fs-extra": "^9.0.1",
    "helmet": "^4.1.0",
    "iconv-lite": "^0.6.2",
    "ioredis": "^4.17.3",
    "joi": "^17.2.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.3.0",
    "pino": "^6.5.1",
    "pino-http": "^5.2.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.10",
    "telegraf": "^3.38.0",
    "telegraf-inline-menu": "^5.3.0",
    "telegraf-session-local": "^2.0.0",
    "telegraf-stateless-question": "^1.0.0",
    "tsyringe": "^4.3.0",
    "typeorm": "0.2.25"
  }
}
